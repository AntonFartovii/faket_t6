<div class="container">
    <div class="toolbar">
    </div>
    <table class="striped">
        <thead>
        <tr>
            <th>Number</th>
            <th>Id</th>
            <th>Name</th>
            <th>Address</th>
            <th>Phone</th>
        </tr>
        </thead>
        <tbody>
        {{#each users}}
            <tr>
                <td>{{number}}</td>
                <td>{{userId}}</td>
                <td>{{username}}</td>
                <td>{{address}}</td>
                <td>{{phone}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>